<table>
    <thead>
        <th>Name</th>
        <th>Price</th>
        <th></th>
    </thead>
    <tbody>
        {{!-- looping  through array we have in our template file --}}
        {{!-- opening  each loop --}}
        {{#each stocks}}
        {{!-- inside this each loop we have access to each one of our objects --}}
        <tr>
            <td class="name">{{name}}</td>
            <td class="price">{{price}}</td>
            <td>
                {{#if this.favorited }}

                <button disabled>Saved</button>

                {{else}}

                <button class="favorite">Save</button>

                {{/if}}
            </td>
        </tr>


        {{!-- closing  each loop --}}
        {{/each}}

    </tbody>
</table>







